# AI Provider Configuration
AI_PROVIDER=gemini  # Can be 'openai' or 'gemini'
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.5-flash
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4-turbo-preview

# Qdrant Configuration
QDRANT_API_KEY=your_qdrant_api_key_here
QDRANT_HOST=your_qdrant_cluster_url
QDRANT_PORT=6333
QDRANT_HTTPS=true
QDRANT_COLLECTION_NAME=rag_embeddings

# Application Settings
DEBUG_MODE=false
LOG_LEVEL=INFO
MAX_QUERY_LENGTH=1000
RESPONSE_TIMEOUT=30

# Server Settings
SERVER_HOST=0.0.0.0
SERVER_PORT=8000

# Additional Settings for Embedding Generation
COHERE_API_KEY=your_cohere_api_key_here
SOURCE_CONTENT_PATH=./test_data/original_content.json
TARGET_URL=https://your-deployment-url.com